{
    "ENTRIES_CONTAINER": {
        "selector": ".ldst__window"
    },
    "ENTRY": {
        "ROOT": {
            "selector": "div.entry"
        },
        "AVATAR": {
            "selector": ".entry__chara__face > img"
        },
        "ID": {
            "selector": ".entry__link",
            "attribute": "href",
            "regex": "/lodestone/character/(?P<ID>\\d*)/"
        },
        "LANG": {
            "selector": ".entry__chara__lang"
        },
        "NAME": {
            "selector": ".entry__name"
        },
        "RANK": {
            "selector": ".entry__chara_info > .js__tooltip",
            "attribute": "data-tooltip",
            "regex": "/ (?P<RankName>.+)"
        },
        "RANK_ICON": {
            "selector": ".entry__chara_info > .js__tooltip > img",
            "attribute": "src"
        },
        "SERVER": {
            "selector": ".entry__world",
            "regex": "(?P<World>\\w*)[\\s\\x{00A0}]\\((?P<DC>\\w*)\\)"
        }
    },
    "LIST_NEXT_BUTTON": {
        "selector": "ul.btn__pager:nth-child(7) > li:nth-child(4) > a:nth-child(1)",
        "attribute": "href"
    },
    "PAGE_INFO": {
        "selector": "ul.btn__pager > li:nth-child(3)",
        "regex": "Page (?P<CurrentPage>[0-9*]) of (?P<NumPages>[0-9*])"
    },
    "NO_RESULTS_FOUND": {
        "selector": ".parts__zero"
    }
}
